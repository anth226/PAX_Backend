{{> header }}
    <h4 class="bodyheader">
        {{#equals lang "en"}}
            Resource Center List
        {{else}}
            রিসোর্স সেন্টার তালিকা
        {{/equals}}
    </h4>
    <table>
        <thead>
            <tr>
            {{#each headerColumns as | header |}}
                <th class="tableHeader">{{ header }}</th>
            {{/each}}
            </tr>
        </thead>
       <tbody>
            {{#each data as | item |}}
                <tr>
                    {{#each item.keys as | key |}}
                        {{#equals key "sl"}}
                            <td>{{ item.sl }}</td>
                        {{/equals}}
                        {{#equals key "name"}}
                            <td>{{ item.name }}</td>
                        {{/equals}}
                        {{#equals key "name_en"}}
                            <td>{{ item.name_en }}</td>
                        {{/equals}}
                        {{#equals key "name_bn"}}
                            <td>{{ item.name_bn }}</td>
                        {{/equals}}
                        {{#equals key "code"}}
                            <td>{{ item.code }}</td>
                        {{/equals}}
                        {{#equals key "resourcecentertypeId"}}
                            <td>{{ item.resourcecentertypeId }}</td>
                        {{/equals}}
                         {{#equals key "area"}}
                            <td>{{ item.area }}</td>
                        {{/equals}}
                        {{#equals key "caretaker"}}
                            <td>{{ item.caretaker }}</td>
                        {{/equals}}
                        {{#equals key "address"}}
                            <td>{{ item.address }}</td>
                        {{/equals}}
                        {{#equals key "status"}}
                            <td>{{ item.status }}</td>
                        {{/equals}}
                    {{/each}}
                </tr>
            {{/each}}
        </tbody>
    </table>
 {{> footer }}